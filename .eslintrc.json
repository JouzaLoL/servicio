{
    "extends": "google",
    "parserOptions": {
        "ecmaVersion": 6
    },
    "rules": {
        "space-before-function-paren": "off",
        "eol-last": "off",
        "no-var": "off",
        "comma-dangle": "off",
        "new-cap": "off",
        "max-len": "off",
        "valid-jsdoc": "off",
        "quotes": 0,
        "no-shadow": "error"
    }
}